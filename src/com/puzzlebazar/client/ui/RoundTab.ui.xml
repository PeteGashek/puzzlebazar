<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:with field='resources' type='com.puzzlebazar.client.resources.Resources' />

	<g:HTMLPanel addStyleNames="{resources.style.roundtab} {resources.style.roundtab_inactive}"
		ui:field="sampleTab">
		<div class="{resources.style.round4}" />
		<div class="{resources.style.round2}" />
		<div class="{resources.style.round1}" />
		<div class="{resources.style.roundtab_inner}">
			<g:Hyperlink ui:field="hyperlink" />
		</div>
	</g:HTMLPanel>

</ui:UiBinder>